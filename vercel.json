{
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/data/(.*)", "destination": "/data/$1" },
    { "source": "/app/(.*)", "destination": "/app/$1" },
    { "source": "/((?!app/|assets/|data/).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=0" }
      ]
    }
  ]
}
